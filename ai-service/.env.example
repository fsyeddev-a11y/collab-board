# OpenRouter — get your key at https://openrouter.ai/keys
OPENROUTER_API_KEY=sk-or-...

# Optional: override the default model (any OpenRouter model slug).
# Default: google/gemini-2.0-flash-exp:free
OPENROUTER_MODEL=google/gemini-2.0-flash-exp:free

# LangSmith tracing (LangChain.js reads the LANGCHAIN_* variants).
# Get your key at https://smith.langchain.com
# LANGCHAIN_API_KEY=ls__...
# LANGCHAIN_TRACING_V2=true
# LANGCHAIN_PROJECT=collabboard
LANGCHAIN_API_KEY=ls__...
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=collabboard

# Shared secret — must match AI_SERVICE_SECRET in the CF Worker.
INTERNAL_SECRET=dev-internal-secret-change-in-prod

# Port the server listens on.
PORT=3001
